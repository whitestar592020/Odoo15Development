<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="standard_cv_view_calendar" model="ir.ui.view">
            <field name="name">standard.cv.calendar</field>
            <field name="model">standard.cv</field>
            <field name="arch" type="xml">
                <calendar date_start="create_date"
                          mode="month"
                          quick_add="0"
                          event_open_popup="1"
                          delete="0"
                          color="gender">

                    <field name="name" string="Career Name"/>
                    <field name="apply_job" string="Apply Job"/>
                    <field name="salary" string="Expect Salary"/>

                    <field name="gender" string="Gender" filters="1" invisible="1"/>
                </calendar>
            </field>
        </record>
    </data>
</odoo>
