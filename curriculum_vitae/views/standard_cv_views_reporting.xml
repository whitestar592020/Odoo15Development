<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="action_standard_cv_analysis" model="ir.actions.act_window">
            <field name="name">Standard CV Analysis</field>
            <field name="res_model">standard.cv</field>
            <field name="view_mode">graph,pivot,calendar</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_empty_folder">
                    No data for analysis!
                </p>
            </field>
        </record>

        <menuitem id="first_level_menu_reporting"
                  name="Reporting"
                  parent="root_menu_cv_form"
                  sequence="10"/>

        <menuitem id="action_menu_standard_cv_analysis"
                  name="Standard CV Analysis"
                  parent="first_level_menu_reporting"
                  action="action_standard_cv_analysis"/>
    </data>
</odoo>
